{
  "name": "tomlin-web",
  "version": "2.3.0",
  "description": "tomlin.no website",
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "./node_modules/.bin/eslint *.js \"app/**/*.js\" \"app/**/*.jsx\"",
    "lint-fix": "./node_modules/.bin/eslint --fix *.js \"app/**/*.js\" \"app/**/*.jsx\"",
    "test": "webpack-dev-server --open --hot --inline --history-api-fallback --config webpack.dev.js",
    "build": "webpack --progress --colors --config webpack.dev.js",
    "build-prod": "export NODE_ENV=production && webpack -p --progress --colors --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/benct/tomlin-web.git"
  },
  "keywords": [
    "ben",
    "tomlin",
    "benct",
    "bct",
    "tomlin.no",
    "react",
    "web",
    "home"
  ],
  "author": "Ben Christopher Tomlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benct/tomlin-web/issues"
  },
  "homepage": "https://github.com/benct/tomlin-web#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-router": "^3.2.0",
    "react-transition-group": "^1.2.1",
    "whatwg-fetch": "^3.0.0"
  }
}
