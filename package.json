{
  "name": "tomlin-web",
  "version": "2.8.0",
  "description": "tomlin.no website",
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "./node_modules/.bin/eslint *.js \"app/**/*.js\" \"app/**/*.jsx\"",
    "lint-fix": "./node_modules/.bin/eslint --fix *.js \"app/**/*.js\" \"app/**/*.jsx\"",
    "test": "webpack-dev-server --open --inline --history-api-fallback --config webpack.dev.js",
    "build": "webpack --progress --colors --config webpack.dev.js",
    "build-prod": "export NODE_ENV=production && webpack -p --progress --colors --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/benct/tomlin-web.git"
  },
  "keywords": [
    "ben",
    "tomlin",
    "benct",
    "bct",
    "tomlin.no",
    "react",
    "web",
    "home"
  ],
  "author": "Ben Christopher Tomlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benct/tomlin-web/issues"
  },
  "homepage": "https://github.com/benct/tomlin-web#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^2.0.1",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^1.15.3",
    "redux-devtools-extension": "^2.13.7",
    "style-loader": "^0.23.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "date-fns": "^2.0.0-alpha.26",
    "iso-639-1": "^2.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-vis": "^1.11.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  }
}
